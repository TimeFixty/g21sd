	--Money Maker Hub (Fizly)
	Money_Maker_Hub_Fizly = function()
		local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()

		local Window = Rayfield:CreateWindow({
			Name = "Soon",
			LoadingTitle = "Coming Soon",
			LoadingSubtitle = "by Time Fixty",
			ConfigurationSaving = {
				Enabled = true,
				FolderName = nil, -- Create a custom folder for your hub/game
				FileName = "Big Hub"
			}
		})
		Rayfield:Notify({
			Title = "Notification Title",
			Content = "Notification Content",
			Duration = 6.5,
			Image = 4483362458,
			Actions = { -- Notification Buttons
				Ignore = {
					Name = "Okay i wait then",
					Callback = function()
						print(":) Maybe In Christmas")
					end
				},
			},
		})
		wait(7)
	end



