		--Destroy Gui
	function Destroy_Gui()
		local Tab2 = Window:CreateTab("Destroy Gui", 4483362458) -- Title, Image
		local Section2 = Tab2:CreateSection("Destroy Gui")

		local Button = Tab2:CreateButton({
			Name = "Destroy Gui",
			Callback = function()

				Rayfield:Notify({
					Title = "Are you Sure",
					Content = "Are you Sure to Destroy Gui?",
					Duration = 6.5,
					Image = 4483362458,
					Actions = { -- Notification Buttons
						Ignore = {
							Name = "Im Sure",
							Callback = function()
								Rayfield:Destroy()
							end
						},
					},
				})
			end,
		})
	end
	
	--Info
	
	function Info()
		local Tab3 = Window:CreateTab("Info", 4483362458) -- Title, Image
		local Section3 = Tab3:CreateSection(" Info")
		
		local Paragraph = Tab3:CreateParagraph({Title = "soon", Content = "Soon!"})
	end
	
	--Credits
	
	function Credits_()
		local Tab4 = Window:CreateTab("Credits", 4483362458) -- Title, Image
		local Section4 = Tab4:CreateSection("Credits")
		local Paragraph = Tab4:CreateParagraph({Title = "soon", Content = "Soon!"})
		
	end










-----------Load Function
function LoadFunction()
	
ComingInFuture()
Notes()
LoadAutoStrat_1()
Destroy_Gui()
Info()
Credits_()

end















