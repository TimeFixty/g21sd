function LoadFizly_Hub()

	local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()
	
	
	local Window = Rayfield:CreateWindow({
		Name = "Fizly Hub",
		LoadingTitle = "Welcome To Fizly Hub v.1.0",
		LoadingSubtitle = "by Time Fixty#1570",
		ConfigurationSaving = {
			Enabled = true,
			FolderName = "Fizly Hub",
			FileName = "Fizly Hub File"
		},
		Discord = {
			Enabled = false,
			Invite = "", 
			RememberJoins = false
	
		},
		KeySystem = true, 
		KeySettings = {
			Title = "Fizly Hub Checking",
			Subtitle = "Key System",
			Note = "Join the discord for Key (soon) ",
			SaveKey = false,
			GrabKeyFromSite = false,
			Key = "ImUsingIt"
	
	
		
		}
	})
