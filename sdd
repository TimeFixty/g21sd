	--Patched
		function Patched()
      
			Rayfield:Notify({
				Title = "Patched",
				Content = "Our Script got Patched or We Adding something Please Wait Until we end fixing it.",
				Duration = 6.5,
				Image = 4483362458,
			})
                  wait(10)
				  Rayfield:Destroy()


		end
		
		--OutDated
		function OutDated()
			Rayfield:Notify({
				Title = "OutDated",
				Content = "This is OutDated Version We copied you link to discord This version will get deleted soon",
				Duration = 6.5,
				Image = 4483362458,
				
			})


			
		end
		--UnsupportVersion
		function UnsupportVersion()  
			Rayfield:Notify({
				Title = "Deleted",
				Content = "This version got deleted because it was old version Pls Join to the discord (link)",
				Duration = 6.5,
				Image = 4483362458,
				
			})
			Rayfield:Destroy()  
			
		end
		
         --Beta
 function BetaVersion()
	
 end

			



	
